#%RAML 1.0
title: UserRegistration
version: 1.0
baseUri: http://localhost:9321/user/{version}
/userreg:
  displayName: Register User
  post:
    description: Register User
    body:
      application/xml:
        schema: !include Account.xsd
    responses:
      200:
        body:
          application/xml:
            schema: !include Account.xsd